{
	"schemaVersion": 1,
	"id": "rimurucraft",
	"version": "${version}",
	"name": "RimuruCraft",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Haywirehax"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/rimurucraft/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.haywirehax.rimurucraft.RimuruCraft",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.haywirehax.rimurucraft.RimuruCraftClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.haywirehax.rimurucraft.RimuruCraftDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"rimurucraft.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.10"
	},
	"suggests": {
		"another-mod": "*"
	}
}